{
  "name": "gym-timer",
  "version": "1.0.0",
  "description": "Gym Timer - Interval training and stopwatch PWA",
  "private": true,
  "type": "module",
  "scripts": {
    "dev-called-from-just": "node scripts/generate-version.js && vite",
    "build-called-from-just": "node scripts/generate-version.js && tsc -b && vite build",
    "test-called-from-just": "vitest run",
    "dev": "just dev",
    "build": "just build",
    "test": "just test",
    "preview": "vite preview",
    "test:watch": "node scripts/generate-version.js && vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "icons": "node scripts/generate-icons.js",
    "generate-version": "node scripts/generate-version.js"
  },
  "keywords": [
    "timer",
    "gym",
    "fitness",
    "pwa",
    "interval-training"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@idvorkin/pwa-utils": "github:idvorkin/pwa-utils",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24.10.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "@vitejs/plugin-react": "^4.5.2",
    "jsdom": "^26.1.0",
    "playwright": "^1.57.0",
    "sharp": "^0.34.5",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^3.2.4"
  }
}
